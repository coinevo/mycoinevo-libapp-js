{
  "name": "mycoinevo-libapp-js",
  "version": "1.0.0",
  "description": "The JS library containing the JS transpilation of the shared library behind the MyCoinevo apps",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mycoinevo/mycoinevo-libapp-js.git"
  },
  "scripts": {
    "format": "find . -name '*.js*' | xargs prettier --write --config ./.prettierrc --config-precedence file-override",
    "test": "mocha"
  },
  "keywords": [
    "coinevo",
    "mycoinevo",
    "javascript",
    "js",
    "lightweight",
    "lightwallet",
    "wallet",
    "crypto",
    "script",
    "utility",
    "tool"
  ],
  "author": "MyCoinevo",
  "license": "See LICENSE.TXT. Copyright (c) 2014-2019, MyCoinevo.tech. All rights reserved.",
  "bugs": {
    "url": "https://github.com/mycoinevo/mycoinevo-libapp-js/issues"
  },
  "homepage": "https://github.com/mycoinevo/mycoinevo-libapp-js#readme",
  "dependencies": {},
  "devDependencies": {
    "mocha": "5.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "node_modules",
      "coinevo_utils/MyCoinevoLibAppCpp_WASM.js",
      "coinevo_utils/MyCoinevoLibAppCpp_ASMJS.js",
      "coinevo_utils/MyCoinevoLibAppCpp_ASMJS.asm.js"
    ]
  }
}
